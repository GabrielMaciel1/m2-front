{
  "expo": {
    "name": "m2-front",
    "slug": "m2-front",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#18181b"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/dark-logo.png",
        "backgroundColor": "#ffffff"
      }
    },
    "plugins": [
      "expo-sqlite"
    ]
  }
}
